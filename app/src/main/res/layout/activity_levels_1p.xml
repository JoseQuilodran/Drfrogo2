<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".levels_1p"
    android:background="@drawable/gree">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="133dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/n1" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="50dp"
        android:layout_height="49dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="133dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/n2" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="48dp"
        android:layout_height="50dp"
        android:layout_marginTop="133dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/n3"
        app:layout_constraintRight_toLeftOf="@+id/imageButton7"
        android:layout_marginRight="33dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="50dp"
        android:layout_height="49dp"
        android:layout_marginTop="133dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/n4"
        android:layout_marginRight="33dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="53dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/n5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        app:layout_constraintHorizontal_bias="0.501" />

    <ImageButton
        android:id="@+id/homel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/homeAsUpIndicator" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/drfrogo"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="-29dp" />
</android.support.constraint.ConstraintLayout>
